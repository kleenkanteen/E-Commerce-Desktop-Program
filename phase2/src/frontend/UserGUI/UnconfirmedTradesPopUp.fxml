      xmlns:fx="http://javafx.com/fxml/1">
   <Label fx:id="unconfirmedTradesPrompt">
      <VBox.margin>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
      </VBox.margin>
      <padding>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
      </padding>
   </Label>
   <Label fx:id="viewItem" prefHeight="197.0" prefWidth="296.0" textAlignment="CENTER">
      <VBox.margin>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
      </VBox.margin>
      <padding>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
      </padding>
   </Label>
   <Button fx:id="confirm" mnemonicParsing="false">
      <VBox.margin>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
      </VBox.margin>
   </Button>
   <Button fx:id="deny" mnemonicParsing="false">
      <VBox.margin>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
      </VBox.margin>
   </Button>
   <Button fx:id="exit" mnemonicParsing="false" prefHeight="34.0" prefWidth="59.0">
      <VBox.margin>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
      </VBox.margin>
   </Button>
   <Label fx:id="allDonePrompt" textFill="#4eaa24">
      <VBox.margin>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
      </VBox.margin>
      <padding>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
      </padding>
   </Label>
</VBox>
