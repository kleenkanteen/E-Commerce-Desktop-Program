<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontend.AdminGUI.AdminAccountController">
   <children>
      <Button fx:id="backFromCreationButton" layoutX="354.0" layoutY="350.0" mnemonicParsing="false" onAction="#BackFromCreationButtonPushed" text="Back to Admin account menu" />
      <Label fx:id="resultOfCreationLabel" layoutX="100.0" layoutY="45.0" />
      <VBox layoutX="100.0" layoutY="73.0" prefHeight="200.0" prefWidth="400.0" spacing="10.0">
         <children>
            <Label text="New Admin's UserName" />
            <TextField fx:id="newAdminUserNameTextField" />
            <Label text="New Admin's Password" />
            <TextField fx:id="newAdminPasswordTextField" />
            <Button fx:id="addNewAdminButton" mnemonicParsing="false" onAction="#addNewAdminButtonPushed" text="Create a new Admin!" />
         </children>
      </VBox>
   </children>
</AnchorPane>
