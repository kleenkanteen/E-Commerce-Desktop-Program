<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="frontend.AdminController">
   <children>
      <VBox layoutX="133.0" layoutY="81.0" prefHeight="238.0" prefWidth="313.0" spacing="10.0">
         <children>
            <Label text="GO Admin! Choose your options below" textAlignment="CENTER" textFill="#7772ba">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Button fx:id="messageInboxButton" mnemonicParsing="false" onAction="#messageInboxButtonPushed" prefHeight="50.0" prefWidth="300.0" text="Check your message inbox" />
            <Button fx:id="manageAdminAccountButton" mnemonicParsing="false" onAction="#manageAdminAccountButtonPushed" prefHeight="50.0" prefWidth="300.0" text="Manage Admin account" />
            <Button fx:id="UserBrowsingButton" mnemonicParsing="false" onAction="#userBrowsingButtonPushed" prefHeight="50.0" prefWidth="300.0" text="Access the information of Users" />
            <Button fx:id="TradeUndoButton" mnemonicParsing="false" onAction="#tradeUndoButtonPushed" prefHeight="50.0" prefWidth="300.0" text="Undo the trade of Users" />
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
