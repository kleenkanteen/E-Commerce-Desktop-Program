<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="183.0" layoutY="14.0" text="Trade" />
      <VBox alignment="CENTER" layoutX="63.0" layoutY="32.0" prefHeight="259.0" prefWidth="275.0">
         <children>
            <DatePicker />
            <TextField promptText="Time of trade (HH:MM format)">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </TextField>
            <TextField prefHeight="26.0" prefWidth="299.0" promptText="meeting of trade">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </TextField>
            <CheckBox mnemonicParsing="false" text="Permanent">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </CheckBox>
            <CheckBox mnemonicParsing="false" text="Temporary" />
            <CheckBox mnemonicParsing="false" text="One way trade">
               <padding>
                  <Insets top="15.0" />
               </padding>
            </CheckBox>
            <CheckBox mnemonicParsing="false" text="Two way trade" />
            <Button mnemonicParsing="false" text="Submit">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
